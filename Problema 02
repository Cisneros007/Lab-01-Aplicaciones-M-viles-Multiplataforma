//Problema 02 
void main() {
    // Declaración de variables de tipo String
    
    // Método 1: Utilizando comillas simples
    String string1 = 'Sebas';
    
    // Método 2: Utilizando comillas dobles
    String string2 = "Sebastian";
    
    // Método 3: Utilizando triple comilla
    String string3 = '''Sebitaas''';
    
    // Declaración de variables numéricas
    // Enteros (int)
    int entero = 42;
    // Flotantes (double)
    double flotante = 3.14;
    
    print("String 1: $string1");
    print("String 2: $string2");
    print("String 3: $string3");
    print("Número entero: $entero");
    print("Número flotante: $flotante");
  }
  